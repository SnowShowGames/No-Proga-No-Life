#include <iostream>
#include <cmath>
#include <clocale>

using namespace std;

bool proverka8(int n1) {
	int k = 0;

	if (n1 == 0) return 0;

	n1 = abs(n1);

	while (n1 > 0) {
		if (n1 % 10 == 8) {
			k++;
			if (k > 3) return 0;
		}
		n1 /= 10;
	}
	return k == 3;
}
int main() {
	setlocale(LC_ALL, "Rus");
	int n;
	cout << "Введите кол-во элементов: " << endl;
	cin >> n;

	int k1 = 0;
	cout << "Введите " << n << " чисел" << endl;
	for (int i = 0; i < n; i++) {
		int n2;
		cin >> n2;
		if (proverka8(n2)) {
			k1++;
		}
	}
	cout << "Результат: " << k1 << endl;
}
